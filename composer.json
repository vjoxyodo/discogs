{
  "name": "vjoxyodo/discogsapi",
  "type": "library",
  "description": "Laravel Library to use Discogs API",
  "keywords":[
    "Discogs",
    "discogs",
    "DiscogsAPI"
  ],
  "homepage":"https://github.com/vjoxyodo/discogs",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Pedro Lopes aka vjoxyodo",
      "email": "vjoxyodo@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2.0"
  },
  "prefer-stable": true,
  "extra":{
    "laravel": {
        "providers": [
            "Vjoxyodo\\DiscogsAPI"
        ]
    }
  },
  "autoload": {
    "psr-4" : {
      "Vjoxyodo\\DiscogsAPI\\" : "src"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "orchestra/testbench": "^6.0"
  }
}

